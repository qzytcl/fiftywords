/* components/DIYInputView/DIYInputView.wxss */
.baseView {
  width: 100%;
  height: 100%;
}
.masking {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.75);
  display: inline-flex;
  align-items: center;
}
.infoBase {
  width: 596rpx;
  margin-left: auto;
  margin-right: auto;
  padding-top: 20rpx;
  padding-bottom: 30rpx;
  background: white;
  border-radius: 6rpx;
}
.noticeClass {
  margin-top: 10rpx;
  margin-left: auto;
  margin-right: auto;
  width: 300rpx;
  text-align: center;
}
.inputC {
  margin-top: 12rpx;
  margin-left: auto;
  margin-right: auto;
  height: 54rpx;
  background-color: white;
  border-style: solid;
  border-width: 1rpx;
  border-color: #ddd;
  border-radius: 6rpx;
  width: 524rpx;
}
.submitBase {
  width: 524rpx;
  margin-top: 90rpx;
  margin-left: auto;
  margin-right: auto;
  color: #fff;
  background-color: #1AAD19;
  font-size: 26rpx;
  border-radius: 6rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  text-align: center;
}
.cancelBase {
  width: 524rpx;
  margin-top: 45rpx;
  margin-left: auto;
  margin-right: auto;
  color: #fff;
  background-color: #999999;
  font-size: 26rpx;
  border-radius: 6rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  text-align: center;
}